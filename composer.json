{
	"name": "bit3/gh-genapidoc-hook",
	"description": "Generate apidoc as service hook and push back as github pages into repository.",
	"keywords": ["github", "apidoc"],
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Tristan Lins",
			"email": "tristan.lins@bit3.de",
			"homepage": "http://bit3.de",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@bit3.de",
		"issues": "https://github.com/bit3/gh-genapidoc-hook/issues",
		"source": "https://github.com/bit3/gh-genapidoc-hook"
	},
	"require": {
		"php": ">=5.3",
		"monolog/monolog": "~1",
		"symfony/yaml": "2.2.*",
		"symfony/process": "2.2.*",
		"symfony/filesystem": "2.2.*",
		"symfony/console": "2.2.*",
		"guzzle/guzzle": "~3"
	},
	"autoload": {
		"psr-0": {
			"GhApiGen": "src"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
